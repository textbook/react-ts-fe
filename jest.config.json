{
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.test.json"
    }
  },
  "moduleNameMapper": {
    "\\.(jpe?g|png|gif|svg)$": "<rootDir>/package.json",
    "\\.(s?css)$": "identity-obj-proxy"
  },
  "preset": "ts-jest",
  "setupFilesAfterEnv": [
    "<rootDir>/setupTests.ts"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/node_modules/"
  ],
  "testRunner": "jest-circus/runner"
}
