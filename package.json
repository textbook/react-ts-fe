{
  "name": "react-ts-fe",
  "version": "0.0.1",
  "description": "Bootstrap for a React TypeScript front-end",
  "main": "src/index.tsx",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "webpack --config webpack/prod.config.ts",
    "deploy:cf": "npm run build && cf push",
    "deploy:heroku": "git push heroku main",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir ./dist",
    "dev": "webpack-dev-server --config webpack/dev.config.ts",
    "e2e": "concurrently -k -s first -n \"app,sln,e2e\" \"npm run dev\" \"npm run selenium\" \"npm run e2e:run\"",
    "pree2e:run": "wait-on tcp:4444 -t 30000 && wait-on tcp:3000 -t 30000",
    "e2e:run": "cross-env JEST_JUNIT_OUTPUT_DIR=reports/e2e jest --testEnvironment node --testRegex e2e-spec\\.ts --testTimeout=30000",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx",
    "preselenium": "webdriver-manager update --gecko false",
    "selenium": "webdriver-manager start",
    "ship": "npm run lint && npm run test && npm run e2e",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/react-ts-fe.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Jonathan Sharpe <mail@jonrshar.pe>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/react-ts-fe/issues"
  },
  "homepage": "https://github.com/textbook/react-ts-fe#readme",
  "dependencies": {
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-merge": "^4.1.5",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "node-sass": "^4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "devDependencies": {
    "@codeyourfuture/eslint-config-standard": "^2.0.2",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-circus": "^26.3.0",
    "jest-junit": "^11.1.0",
    "ts-jest": "^25.5.1",
    "wait-on": "^5.2.0",
    "webdriver-manager": "^12.1.7",
    "webdriverio": "^6.4.0"
  }
}
