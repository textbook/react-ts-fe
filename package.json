{
  "name": "react-ts-fe",
  "version": "0.0.1",
  "description": "Bootstrap for a React TypeScript front-end",
  "main": "src/index.tsx",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "webpack --config webpack/prod.config.ts",
    "deploy:cf": "npm run build && cf push",
    "deploy:heroku": "git push heroku main",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir ./dist",
    "dev": "webpack serve --config webpack/dev.config.ts",
    "e2e": "concurrently -k -s first -n \"app,sln,e2e\" \"npm run dev\" \"npm run selenium\" \"npm run e2e:safe\"",
    "e2e:run": "cross-env JEST_JUNIT_OUTPUT_DIR=reports/e2e jest --testEnvironment node --testRegex e2e-spec\\.ts --testTimeout=30000",
    "e2e:safe": "wait-on tcp:4444 -t 30000 && wait-on tcp:3000 -t 30000 && npm run e2e:run",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx",
    "preselenium": "webdriver-manager update --gecko false",
    "selenium": "webdriver-manager start",
    "ship": "npm run lint && npm run test && npm run e2e",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/react-ts-fe.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Jonathan Sharpe <mail@jonrshar.pe>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/react-ts-fe/issues"
  },
  "homepage": "https://github.com/textbook/react-ts-fe#readme",
  "dependencies": {
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/node": "^15.14.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/webpack-dev-server": "^3.11.4",
    "@types/webpack-merge": "^5.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "html-webpack-tags-plugin": "^3.0.1",
    "node-sass": "^6.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@codeyourfuture/eslint-config-standard": "^2.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.0.6",
    "jest-junit": "^12.2.0",
    "ts-jest": "^26.5.6",
    "wait-on": "^6.0.0",
    "webdriver-manager": "^12.1.8",
    "webdriverio": "^7.7.4"
  }
}
